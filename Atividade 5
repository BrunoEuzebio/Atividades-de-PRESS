#include <iostream>
#include <cstdlib>

using namespace std;

int main(int argc, char** argv) {

    int num;
    
    cout << "Insira um numero qualquer :";
    cin >> num;
    
    if (num >= 0)
        
        cout << "O numero é positivo !";
    else 
        
        cout << "O numero é negativo!";
    return 0;
}

